<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Birth place</th>
      <th>Birth date</th>
      <th>Nickname</th>
      <th>Living place</th>
      <th>Gender</th>
      <th>Gender identity</th>
      <th>Sexuality</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.birth_place %></td>
        <td><%= profile.birth_date %></td>
        <td><%= profile.nickname %></td>
        <td><%= profile.living_place %></td>
        <td><%= profile.gender %></td>
        <td><%= profile.gender_identity %></td>
        <td><%= profile.sexuality %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
